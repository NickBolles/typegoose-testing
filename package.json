{
  "name": "@hasezoey/verify-typegoose-issues",
  "version": "0.0.1",
  "description": "Bootstrap TS Project All Files needed for TS",
  "main": "lib/app.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "files": [
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "scripts": {
    "start": "npm run build && npm run run",
    "run": "ts-node ./src/test.ts",
    "watch": "tsc -w",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "doc": "typedoc --out ./doc ./src --mode modules",
    "clean": "rm -rf ./doc ./lib ./coverage ./nyc_output",
    "prepublish": "npm run build"
  },
  "repository": "none",
  "author": "hasezoey",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/node": "^10.14.17",
    "@types/source-map-support": "^0.5.0",
    "@types/mongoose": "^5.5.18",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^6.0.0",
    "debug": "^4.1.1",
    "mongoose": "^5.7.3",
    "source-map-support": "^0.5.13"
  }
}
